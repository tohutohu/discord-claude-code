{
  "tasks": {
    "start": "deno run -A cli.ts run",
    "dev": "deno run -A --watch cli.ts run",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "check": "deno check --allow-import **/*.ts",
    "test": "deno test -A",
    "cov": "deno test -A --coverage=coverage",
    "cov:html": "deno coverage coverage --html",
    "install-hooks": "deno run -A scripts/install-hooks.ts"
  },
  "imports": {
    "@cliffy/command": "https://deno.land/x/cliffy@v1.0.0-rc.4/command/mod.ts",
    "@cliffy/prompt": "https://deno.land/x/cliffy@v1.0.0-rc.4/prompt/mod.ts",
    "@cliffy/table": "https://deno.land/x/cliffy@v1.0.0-rc.4/table/mod.ts",
    "@deno/tui": "https://deno.land/x/tui@2.1.5/mod.ts",
    "@std/assert": "https://deno.land/std@0.224.0/assert/mod.ts",
    "@std/async": "https://deno.land/std@0.224.0/async/mod.ts",
    "@std/datetime": "https://deno.land/std@0.224.0/datetime/mod.ts",
    "@std/encoding": "https://deno.land/std@0.224.0/encoding/mod.ts",
    "@std/fmt": "https://deno.land/std@0.224.0/fmt/mod.ts",
    "@std/fs": "https://deno.land/std@0.224.0/fs/mod.ts",
    "@std/path": "https://deno.land/std@0.224.0/path/mod.ts",
    "@std/testing": "https://deno.land/std@0.224.0/testing/mod.ts",
    "@std/yaml": "https://deno.land/std@0.224.0/yaml/mod.ts",
    "@discordeno/bot": "https://deno.land/x/discordeno@19.0.0-alpha.14/mod.ts",
    "@discordeno/rest": "https://deno.land/x/discordeno@19.0.0-alpha.14/rest/mod.ts",
    "@discordeno/utils": "https://deno.land/x/discordeno@19.0.0-alpha.14/utils/mod.ts",
    "react": "npm:react@18.3.1",
    "react/jsx-runtime": "npm:react@18.3.1/jsx-runtime"
  },
  "fmt": {
    "include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx", "**/*.json", "**/*.md"],
    "exclude": ["coverage/", "dist/", "node_modules/"],
    "lineWidth": 100,
    "indentWidth": 2,
    "singleQuote": true,
    "proseWrap": "preserve"
  },
  "lint": {
    "include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx"],
    "exclude": ["coverage/", "dist/", "node_modules/"],
    "rules": {
      "tags": ["recommended"],
      "include": ["ban-untagged-todo", "no-explicit-any"],
      "exclude": ["no-unused-vars"]
    }
  },
  "test": {
    "include": ["**/*.test.ts", "**/*_test.ts"],
    "exclude": ["coverage/", "dist/", "node_modules/"]
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "npm:@types/react@18.3.1"
  }
}
